package me.helldiner.holdon.hook;

public class Injector {
	
	private boolean success;
	
	public Injector(String dllDir, int pid) {
		this.success = this.inject(dllPath, pid);
	}
	
	public boolean wasDllInjected() {
		return this.success;
	}
	
	public void inject(String dllName) {
		this.inject(dllPath, pid)
	}
	
	private native boolean inject(String dllPath, int pid);
	
}